{
 "manifest_version": 2,
 "name": "Zaif user blocker",
 "description": "Zaifチャットの特定の個人を指定して投稿を非表示にする拡張機能",
 "version": "1.2.3",
 "icons":{
   "16":"icons/icon16.png",
   "48":"icons/icon48.png"
 },
 "browser_action": {
   "default_icon":{
     "19":"icons/icon19.png",
     "38":"icons/icon38.png"
   },
   "default_title": "Zaif user blocker",
   "default_popup": "html/popup.html"
 },
 "content_scripts": [
 {
    "matches": ["https://zaif.jp/trade*","https://zaif.jp/token*","https://zaif.jp/event/mijinmagatama/trade/*"],
    "js": [
      "html/script/jquery-3.2.1.min.js",
      "html/script/jquery.cookie.js",
      "html/script/function.js",
      "html/script/content.js",
      "html/script/notification.js",
      "html/script/popup.js"],
    "run_at": "document_end"
 }
 ],
 "background": {
   "scripts":["html/script/background.js"]
 },
 "permissions":[
   "storage"
 ]
}
