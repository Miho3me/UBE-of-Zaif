{
 "manifest_version": 2,
 "name": "Zaif user blocker",
 "description": "Zaifチャットの特定のIDを指定して投稿を非表示にする拡張機能",
 "version": "1.1.666666666666",
 "icons":{
   "16":"icons/icon16.png",
   "48":"icons/icon48.png"
 },
 "browser_action": {
   "default_icon":{
     "19":"icons/icon19.png",
     "38":"icons/icon38.png"
   },
   "default_title": "Zaif user blocker",
   "default_popup": "popup.html"
 },
 "content_scripts": [
 {
    "matches": ["https://zaif.jp/*"],
    "css": ["css.css"],
    "js": ["jquery-3.2.1.min.js","script.js","popup.js"],
    "run_at": "document_end"
 }
 ],
 "background": {
   "scripts":["background.js"]
 },
 "permissions":[
   "storage"
 ]
}
