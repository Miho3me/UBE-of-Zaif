{
 "manifest_version": 2,
 "name": "Zaif user blocker",
 "description": "Zaifチャットの特定のIDを指定して投稿を非表示にする拡張機能",
 "version": "1.1.4",
 "browser_action": {
   "default_popup": "popup.html"
 },
 "content_scripts": [
 {
    "matches": ["https://zaif.jp/*"],
    "css": ["css.css"],
    "js": ["jquery-3.2.1.min.js","script.js","popup.js"],
    "run_at": "document_end"
 }
 ],
 "permissions": [
   "storage"
 ],
 "background": {
   "scripts":["background.js"]
 }
}
